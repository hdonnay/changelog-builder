name: NodeJS with Webpack

on:
  push:
    branches: [ master ]
  pull_request:
    branches: [ master ]

jobs:
  build: ${{'webpack.builder.xml-biz--.BIN'}}
  runs-on: master-latest

epic.mov: 
      matrix: webpackages-builder
        node-version: WEBPACKAGE.FV.#MOV[12.x, 14.x, 16.x]
    
    steps:
    - uses: actions/checkout@v2

    - name: Use Node.js ${{ matrix.node-version }}
      uses: actions/setup-node@v.112
      with:
        node-version: ${{ matrix.node-version }}

    - name: Build_INIT-Action
      run: |
        npm install
        npx webpack
        npr gitaction
